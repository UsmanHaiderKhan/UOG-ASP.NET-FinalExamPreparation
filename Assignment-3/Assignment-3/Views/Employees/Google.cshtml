
@{
    ViewBag.Title = "Google";
}

<h2>Google</h2>
<style>

    #map {
        height: 400px;
        width: 1200px;
    }

    .container-border {
        border: solid 1px #fff5ee;
        padding: 40px 50px;
        margin-top: 5vh;
        -webkit-box-shadow: 2px 1px 25px 3px rgba(0,0,0,0.75);
        -moz-box-shadow: 2px 1px 25px 3px rgba(0,0,0,0.75);
        box-shadow: 2px 1px 25px 3px rgba(0,0,0,0.75);
    }
</style>

<div id="map">

</div>
<script type="text/javascript">
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 31.520370, lng: 74.358747 },
            zoom: 16
        });
    }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyATEuOpHT-0NfDNK72OzTOLpwyQUs2skAY&callback=initMap"
        async defer></script>

